<div class="contenedorCards">
</div>

<form id="formProducts" onsubmit="return agregarProducto(this)">
  <label for="producto"> Producto: </label>
  <input type="text" id="producto" required />
  <br /><br />

  <label for="descripcion"> Descripcion: </label>
  <input type="text" id="descripcion" required />
  <br /><br />

  <label for="precio"> Precio: </label>
  <input type="number" id="precio" required />
  <br /><br />

  <label for="imagen"> Imagen: </label>
  <input type="text" id="imagen" required />
  <br /><br />

  <label for="codigo"> Codigo: </label>
  <input type="number" id="codigo" required />
  <br /><br />

  <label for="stock"> Stock: </label>
  <input type="stock" id="stock" required />
  <br /><br />

  <button type="submit"> Nuevo Producto </button>
</form>

